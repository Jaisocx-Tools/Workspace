/* Responsive rules */
@media only screen and (min-width: {{ minWidth }}px) and (max-width: {{ maxWidth }}px) and (orientation: {{ orientation }}) {

  .workspace {
    {{ deviceSizeConstantLine }}

    {{ deviceSizeConstant_minWidth }}
    {{ deviceSizeConstant_maxWidth }}
  }

  /* verifying responsive rules applies */
  .workspace.verify-responsive-rule-applies main h1::before {
    content: var(--device_size_name);
  }

}

